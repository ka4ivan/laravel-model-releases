{
    "name": "ka4ivan/laravel-model-releases",
    "description": "Model Releases (versions) for Laravel Framework",
    "keywords": [
        "laravel", "model", "release", "version", "versioning", "deploy", "draft", "revision", "kachula"
    ],
    "authors": [
        {
            "name": "Kachula Ivan",
            "email": "ka4ivan.dev@gmail.com",
            "role": "Architect & Developer",
            "homepage": "https://github.com/ka4ivan"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Ka4ivan\\ModelReleases\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ka4ivan\\ModelReleases\\ServiceProvider"
            ],
            "aliases": {
                "ModelRelease": "Ka4ivan\\ModelReleases\\Facades\\ModelRelease"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
